<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
	<servlet>
		<servlet-name>createsystem</servlet-name>
		<servlet-class>solarcalculator.CreateSystem</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>locationinput</servlet-name>
		<servlet-class>solarcalculator.LocationInput</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>panelsinput</servlet-name>
		<servlet-class>solarcalculator.PanelsInput</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>panelsconfig</servlet-name>
		<servlet-class>solarcalculator.PanelsConfig</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>showresults</servlet-name>
		<servlet-class>solarcalculator.ShowResults</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>resetdata</servlet-name>
		<servlet-class>solarcalculator.ResetData</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>createsystem</servlet-name>
		<url-pattern>/createsystem</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>locationinput</servlet-name>
		<url-pattern>/locationinput</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>panelsinput</servlet-name>
		<url-pattern>/panelsinput</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>panelsconfig</servlet-name>
		<url-pattern>/panelsconfig</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>showresults</servlet-name>
		<url-pattern>/showresults</url-pattern>
	</servlet-mapping>	
	
	<servlet-mapping>
		<servlet-name>resetdata</servlet-name>
		<url-pattern>/resetdata</url-pattern>
	</servlet-mapping>	
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	
 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value/>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
</web-app>